    "HTTP",
    "MIDI",
    "Converter"
  ],
  "author": "Josia Scheerle",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "easymidi": "^2.1.0",
    "express": "^4.17.3",
    "nodemon": "^2.0.15",
    "url-parse": "^1.5.4",
    "pkg": "^5.6.0"
  },
  "pkg": {
    "assets": ["views/**/*", "scripts/**/*"],
    "targets": [ "node16-win-x64" ]
  }
