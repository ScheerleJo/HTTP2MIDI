{
  "name": "http2midi",
  "version": "1.4.2",
  "description": "A Webserver, which accepts HTTP-Requests and outputs MIDI Signals",
  "main": "index.js",
  "scripts": {
    "startup": "node index.js",
    "build": "pkg package.json --compress GZip"
  },
  "bin": {
    "http2midi": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ScheerleJo/http2midi/"
  },
  "keywords": [
    "HTTP",
    "MIDI",
    "Converter"
  ],
  "author": "Josia Scheerle",
  "license": "MIT",
  "dependencies": {
    "easymidi": "^3.1.0",
    "express": "^4.21.0",
    "find-process": "^1.4.7",
    "nconf": "^0.12.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "eslint": "^8.15.0"
  },
  "pkg": {
    "assets": [
      "scripts/**/*",
      "config/**/*",
      "node_modules/**/*"
    ],
    "targets": [
      "node24-win-x64"
    ]
  }
}
