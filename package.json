{
  "name": "http2midi",
  "version": "1.0.0",
  "description": "A Webserver, which accepts HTTP-Requests and outputs MIDI CC-Commands",
  "main": "index.js",
  "scripts": {
    "startup": "node index.js",
    "dev": "nodemon index.js"
  },
  "bin":{
    "http2midi": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "HTTP2MIDI"
  },
